# MindsDB Environment Configuration
# Copy this file to .env and fill in your actual credentials
# DO NOT commit the .env file to version control

# MindsDB API Configuration
MINDS_API_KEY=your_mindsdb_api_key_here

# Airtable Integration
AIRTABLE_API_KEY=your_airtable_api_key_here

# Hugging Face Integration
HUGGING_FACE_API_KEY=your_hugging_face_api_key_here

# OpenAI Integration
OPENAI_API_KEY=your_openai_api_key_here

# Supabase PostgreSQL Database
SUPABASE_POSTGRES_URL=postgresql://username:password@host:port/database

# S3/CloudFlare R2 Storage
S3_BUCKET_URL=your_s3_bucket_url_here

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Additional Database Connections
# Add other database connection strings as needed
# MYSQL_URL=mysql://username:password@host:port/database
# MONGODB_URL=mongodb://username:password@host:port/database